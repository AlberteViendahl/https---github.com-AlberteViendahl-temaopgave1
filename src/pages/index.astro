---
import { Image } from "astro:assets";
import BaseLayout from "../layouts/BaseLayout.astro";
import EmployeeSection from "../componentes/EmployeeSection.astro";

/*import daisy from "../assets/images/det billede jeg evt har"*/

const employeeData = await fetch(
  "https://raw.githubusercontent.com/AlberteViendahl/frontend-design/main/resources/json/employees.json"
).then((res) => res.json());
---

<BaseLayout title="Home">
  <Image alt="" src={} />

  <EmployeeSection data="{employeeData}" />

  <BaseLayout title="Home" /></BaseLayout
>
